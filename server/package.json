{
  "name": "joke-factory",
  "version": "1.0.0",
  "description": "joke-factory app. A portfolio project for practicing TDD, TypeScript, and Express",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "start:server": "node dist/index.js",
    "watch": "nodemon dist/index.js",
    "compile:watch": "tsc --watch -p tsconfig.build.json",
    "compile": "tsc -p tsconfig.build.json",
    "prestart": "npm run clean && npm run compile ",
    "start": "npm run compile:watch & npm run watch",
    "deploy": "npm run clean && npm compile && npm start:server",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "lint": "eslint ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.12",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.38.0",
    "express": "^4.18.2",
    "joi": "^17.9.1",
    "sequelize": "^6.31.0",
    "sqlite3": "^5.1.6"
  },
  "prettier": {
    "singleQuote": true
  }
}
